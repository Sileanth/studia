data Unit = ()

data Bool = False | True

data Option X = None | Some of X

data Pair A B = (,) of A, B

data Either L R = Left of L | Right of R

data rec List X = [] | (::) of X, List X

data Ord = LT | EQ | GT

pragma type "UnitType": Unit
pragma type "BoolType": Bool
pragma type "ListType": List
